#include<stdio.h>
void main()
{
    int a[50],n,i,max1,max2;
    printf("enter the size of array");
    scanf("%d",&n);
    printf("enter the element of array");
    for (i=0;i<0;i++)
    {
        scanf("%d",&n);
    }
    max1=-32768;
    max2=-32768;
    for(i=0;i<n;i++)
    {
        if (max1<a[i])
        {
            max2=max1;
            max1=a[i];
        }
        else
        if ((max2<a[i])&&(max1!=a[i]))
        {
            max2=a[i];
        }
    }
    printf("second largest element of an array =%d",max2);
}